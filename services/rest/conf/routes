POST	/api/v1/acl/space				 	@controllers.SpaceController.create()
GET		/api/v1/acl/space/:spaceId			@controllers.SpaceController.get(spaceId)
PUT		/api/v1/acl/space					@controllers.SpaceController.update()
DELETE	/api/v1/acl/space/:spaceId			@controllers.SpaceController.delete(spaceId)
GET		/api/v1/acl/space/					@controllers.SpaceController.listUser()

# Acl/role
GET		/api/v1/acl/roles					@controllers.RoleController.list()
